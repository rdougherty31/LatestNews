<h1>Latest News</h1>
        <button class="notesBtn" onClick="showNotes()" data-state="0">Show Notes</button>
<ul>
    {{#each article}}
    <li>
        <a href={{link}} target="_blank">
            <div>{{title}}</div>
            <div>[ {{link}} ]</div>
            <div class="notesOverlay">
        </a>
    <div class="notes">
        <div>Notes</div>
        <div>{{note}}</div>
    </div>
</div>
    </li>
    {{/each}}
</ul>